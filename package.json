{
  "name": "@parsify/core-helpers",
  "version": "0.0.2-fixbuild2",
  "description": "Bunch of @parsify/core helpers, written in Rust",
  "main": "lib/index.js",
  "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parsify-dev/core-helpers.git"
  },
  "files": [
    "native/index.node",
    "lib"
  ],
  "dependencies": {
    "neon-cli": "^0.4.0",
    "node-pre-gyp": "github:amilajack/node-pre-gyp#neon-compat",
    "node-pre-gyp-github": "^1.4.3"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "package": "node-pre-gyp package",
    "upload-binary": "node-pre-gyp package && node-pre-gyp-github publish"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/parsify-dev/core-helpers/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  }
}
